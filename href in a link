    <a></a>
    <a></a>
    <a></a>
<button>click me</button>
    <script>
    let a = document.querySelectorAll('a');
let btn = document.querySelector('button');
function func () {
   for (let i = 0; i < a.length; i++) {
    a[i].innerHTML = a[i].innerHTML + 'href';
   }
}
btn.addEventListener('click', func);
    </script>   
    
    
</body>
